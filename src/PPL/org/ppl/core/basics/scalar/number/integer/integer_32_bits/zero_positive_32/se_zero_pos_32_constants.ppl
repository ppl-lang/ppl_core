service zero_pos_32_constants

	attributes type:zero_pos_32

		min_value
			default
			    variable r zero_pos_32
                java
                    v_r = new fa_zero_pos_32(0);
                end java
                assert r is not null
                return r
            .
        .

		max_value
			default
			    variable r zero_pos_32
                java
                    v_r = new fa_zero_pos_32 ( Integer.MAX_VALUE );
                end java
                assert r is not null
                return r
            .
        .

		zero
			default
			    variable r zero_pos_32
                java
                    v_r = new fa_zero_pos_32(0);
                end java
                assert r is not null
                return r
            .
        .

		one
			default
			    variable r zero_pos_32
                java
                    v_r = new fa_zero_pos_32(1);
                end java
                assert r is not null
                return r
            .
        .
    .
.
