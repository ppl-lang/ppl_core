template positive_32_value_type_with_maximum_check

	param type_id end
	param default_factory_yes_no end
	param max_value end

	type {type_id} default_factory:{default_factory_yes_no}

		inherit positive_32_value
			redefine
				%ty_pos_32_value_maximum_check < "{max_value}" >
			end
		end

	end
	
end